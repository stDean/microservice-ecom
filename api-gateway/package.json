{
  "name": "api-gateway",
  "version": "1.0.0",
  "main": "index.js",
  "description": "API Gateway for Microservices Architecture",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d --build",
    "docker:dev:logs": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml logs -f",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d --build",
    "docker:down": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down -v",
    "docker:down:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml down -v",
    "docker:build": "docker build -t api-gateway .",
    "test": "vitest",
    "test:integration": "vitest run tests/integration",
    "test:unit": "vitest run tests/unit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "msw": "^2.11.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/nock": "^10.0.3",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^19.0.2",
    "nock": "^14.0.10",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
